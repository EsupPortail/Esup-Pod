      ["id=s", "id"],
      ["name=q", "name"],
      ["css=#s", "css:finder"],
      ["xpath=//input[@id='s']", "xpath:attributes"],
      ["xpath=//form[@id='nav-search']/input", "xpath:idRelative"],
      ["xpath=//input", "xpath:position"]
